@page "/Category/{Id}"

<MudBlazor.MudPaper>
    <MudBlazor.MudText>@Id</MudBlazor.MudText>
    @foreach (var img in imageSources)
    {
        <img src="@img" width="300" height="300"/>
    }
</MudBlazor.MudPaper>

@code {
    [Parameter]
    public string Id { get; set; }
    private List<string> imageSources = new List<string>();

    protected override async Task OnInitializedAsync()
    {
        imageSources = ImageService.Instance.GetImageOfCategory(Id);
    }
}
